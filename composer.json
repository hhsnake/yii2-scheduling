{
  "name": "hhsnake/yii2-scheduling",
  "description": "Scheduling extension for Yii2 framework forked omnilight/yii2-scheduling",
  "keywords": [
    "yii",
    "scheduling",
    "cron"
  ],
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Pavel Agalecky",
      "email": "pavel.agalecky@gmail.com"
    }
  ],
  "type": "yii2-extension",
  "require": {
    "php": ">=7.2.0",
    "yiisoft/yii2": "2.0.*@stable",
    "symfony/process": "2.6.* || 3.* || 4.* || ^5.0|| ^6.0",
    "dragonmantank/cron-expression": "^3.3",
    "guzzlehttp/guzzle": "@stable"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.36"
  },
  "suggest": {
    "guzzlehttp/guzzle": "Required to use the thenPing method on schedules (~5.0)."
  },
  "autoload": {
    "psr-4": {
      "hhsnake\\scheduling\\": "src"
    }
  },
  "extra": {
    "bootstrap": "hhsnake\\scheduling\\Bootstrap"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "license": [
      "MIT"
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
